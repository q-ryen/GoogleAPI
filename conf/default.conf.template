[GoogleAPI]
key_file: key_file.key
client_email: api_email_associated_with_key@developer.gserviceaccount.com
api: webmasters # which_api_endpoint
version: v3 # api_endpoint_version

[scope]
# format for multiple scopes
# number: scope_url
0: https://www.googleapis.com/auth/webmasters
1: https://www.googleapis.com/auth/siteverification.verify_only
2: https://www.googleapis.com/auth/sqlservice.admin

[proxy]
host: proxy.url.com
port: 1234
